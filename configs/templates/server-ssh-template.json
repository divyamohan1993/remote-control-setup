{
  "version": "1.0.0",
  "device": {
    "id": "example-server-001",
    "name": "Example Server",
    "type": "server",
    "description": "Template configuration for server SSH access",
    "tags": ["template", "server", "ssh"]
  },
  "connection": {
    "protocol": "ssh",
    "host": "server.example.com",
    "port": 22,
    "secure": true,
    "authentication": {
      "method": "key",
      "username": "root",
      "requiresCredentials": true
    },
    "proxy": {
      "enabled": false
    }
  },
  "settings": {
    "clipboard": true,
    "fileTransfer": true,
    "customSettings": {
      "terminalType": "xterm-256color",
      "keepAlive": true
    }
  },
  "metadata": {
    "createdAt": "2025-10-17T08:00:00Z",
    "owner": "admin",
    "maintainers": ["admin"]
  }
